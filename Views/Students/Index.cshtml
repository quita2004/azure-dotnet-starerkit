@model IEnumerable<WebApplicationMVC.Models.Student>

@{
    ViewData["Title"] = "Images";
}

<h1>Images</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Section)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Branch)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmailId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Section)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Branch)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmailId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.StudentNo">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.StudentNo">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.StudentNo">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
